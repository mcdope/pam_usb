#!/usr/bin/make -f

%:
	dh $@

override_dh_installinit:
	dh_installsystemd -plibpam-usb --no-restart-on-upgrade --name=pamusb-agent pamusb-agent.service
	dh_installsystemduser -plibpam-usb --name=pamusb-keyring-unlock-gnome pamusb-keyring-unlock-gnome.service
	dh_installinit -plibpam-usb --no-start --noscripts

override_dh_systemd_start:
	echo "Not running dh_systemd_start"
